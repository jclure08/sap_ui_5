<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No User Found"
		title="Select User"
		search="handleUserSelectDialogSearch"
		confirm="handleUserSelectDialogClose"
		cancel="handleUserSelectDialogClose"
		items="{
			path: '/UserSet'
		}" >
		<StandardListItem
			title="{FirstName} {LastName}"
			description="{Id}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>